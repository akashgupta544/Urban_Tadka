    <link href="~/Scripts/bootstrap.css" rel="stylesheet" />
    <link href="~/Home.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap1.js"></script>
    <script type="text/javascript">
        var c = 0;
        $(document).ready(function () {

            if (window.innerWidth > 767) {
                $('.top-bar').hover(function () { $(this).css('background-color', '#F75D59') }).mouseout(function () { $(this).css('background-color', 'white') });
            }
            $('#saver').hide();
            if (window.innerWidth <= 767) {
                $('.top-bar').css('background-color', '#9F000F');
                $('#saver').show();
                $('#menu-icon').click(function () {
                    $('#saver').css('height', '150px')
                    c++

                    if (c % 2 != 0) {
                        $('div[id="locate"]').animate({ 'top': 200 });
                    }
                    else {
                        $('div[id="locate"]').animate({ 'top': 0 });
                        $('#saver').css('height', '30px')
                    }

                })
                if (window.innerWidth <= 1150 && window.innerWidth > 767)
                {
                    $('#locate1').css('margin', '150px');
                }

            }
            
        });
    </script>
</head>
