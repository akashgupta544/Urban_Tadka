            $('#rightappetizer').click(function () {
                countright++;
                $('#Appetizers').each(function () {
                    $(this).animate({ 'marginLeft': "-=300px" }, 400, 'linear');


                })


            })

            $('#leftappetizer').click(function () {
                if (countright != 0) {
                    $('#Appetizers').each(function () {
                        $(this).animate({ 'marginLeft': "+=300px" }, 400, 'linear');

                    })
                    countright--;
                }

            })
            $('#rightcurry').click(function () {
                countright++;
                $('#Curries').each(function () {
                    $(this).animate({ 'marginLeft': "-=300px" }, 400, 'linear');


                })

            })

            $('#leftcurry').click(function () {
                if (countright != 0) {
                    $('#Curries').each(function () {


                        $(this).animate({ 'marginLeft': "+=300px" }, 400, 'linear');

                    })
                    countright--;
                }

            })
            $('#rightdessert').click(function () {
                countright++;
                $('#Dessert').each(function () {
                    $(this).animate({ 'marginLeft': "-=300px" }, 400, 'linear');


                })

            })

            $('#leftdessert').click(function () {
                if (countright != 0) {
                    $('#Dessert').each(function () {

                        $(this).animate({ 'marginLeft': "+=300px" }, 400, 'linear');

                    })
                    countright--;
                }

            })


        });

        function pricecalculator() {
            var appetizercount = 0;
            var totalprice = 0;

            var vegcurrylength;
            var chickencurrylength = 0;
            var goatcurrylength = 0;
            var countupgrade = 3;
            var ricecount = 0;
            var dessertcount = 0;
            if ($('input[id="appetizercheck"]:checked').length == 0) {
                alert("you did not choose any appetizer. Please choose atleast one appetizer");
                return;
            }
            else if ($('input[id="currycheck"]:checked').length < 3) {
                alert("you have to choose atleast three curry to continue. Please call us if have any further question ");
                return;
            }
            else if ($('input[id="dessertcheck"]:checked').length < 1) {

                alert("you have to choose atleast one dessert and one  rice. Please choose one to continue");
                return;
            }
            else if ($('input[name="9"]:checked').length < 1) {
                alert("please select atleast one dessert"); return;
            }
            else {

                $('input[id="appetizercheck"]:checked').each(function () {
                    if ($(this).val() == "Veg Pakora") {
                        appetizercount++;

                    }
                })
                if (appetizercount != 0) {
                    var t = $('input[id="appetizercheck"]:checked').length;
                    if (t > 1)
                    { totalprice = 2 * (t - 1) + 1.66; appetizercount = 0; }
                    else { totalprice = 1.66; appetizercount = 0; }
                }
                else {

                    var t = $('input[id="appetizercheck"]:checked').length;
                    totalprice = 2 * (t - 1) + 2.66;
                }


                vegcurrylength = $('input[name="2"]:checked').length + $('input[name="3"]:checked').length + $('input[name="4"]:checked').length;

                chickencurrylength = $('input[name="6"]:checked').length;
                goatcurrylength = $('input[name="7"]:checked').length;
                if (vegcurrylength >= 3) {
                    totalprice = totalprice + ((3 * 1.66 + (vegcurrylength - 3) * 2) + (chickencurrylength * 2) + (goatcurrylength * 3));
                    chickencurrylength = 0; vegcurrylength = 0; goatcurrylength = 0;

                }
                else {
                    var i = 1;
                    while (i < 4) {
                        if (i == 1) {
                            if (vegcurrylength > 0 && vegcurrylength > countupgrade) {
                                vegcurrylength = vegcurrylength - countupgrade;
                                totalprice = totalprice + (countupgrade * 1.66) + (vegcurrylength * 2);
                                break;
                            }
                            else if (vegcurrylength > 0 && vegcurrylength < countupgrade) {
                                totalprice = totalprice + (vegcurrylength * 1.66);
                                countupgrade = countupgrade - vegcurrylength;

                            }
                            i++;
                        }
                        if (i == 2) {
                            if (chickencurrylength > 0 && chickencurrylength >= countupgrade) {
                                chickencurrylength = chickencurrylength - countupgrade;
                                totalprice = totalprice + (countupgrade * 2.66) + (chickencurrylength * 2);

                                countupgrade = 0;


                            }
                            else if (chickencurrylength > 0 && chickencurrylength < countupgrade) {
                                totalprice = totalprice + (chickencurrylength * 2.66);
                                countupgrade = countupgrade - chickencurrylength;

                            }
                            i++;
                        }
                        if (i == 3) {
                            if (goatcurrylength > 0) {
                                goatcurrylength = goatcurrylength - countupgrade;
                                totalprice = totalprice + (countupgrade * 3.66) + (goatcurrylength * 3);

                            }
                            i++;
                        }
                    }


                }


                ricelength = $('input[name="8"]:checked').length;
                dessertlength = $('input[name="9"]:checked').length;
                $('input[name="8"]:checked').each(function () {
                    if ($(this).val() == "Plain Rice" || $(this).val() == "Jeera Rice") {
                        ricecount++;

                    }
                });
                if (ricecount != 0) {
                    var t = $('input[name="8"]:checked').length;

                    if (t > 1)
                    { totalprice = totalprice + (2 * (t - 1) + 1.66); }
                    else { totalprice = totalprice + 1.66; }
                }
                else {

                    var t = $('input[name="8"]:checked').length;
                    totalprice = totalprice + (2 * (t - 1) + 2.66);

                }

                $('input[name="9"]:checked').each(function () {
                    if ($(this).val() == "Badami Kheer" || $(this).val() == "Gulab Jamun") {
                        dessertcount++;

                    }
                });
                if (dessertcount == 0) {
                    var t = $('input[name="9"]:checked').length;
                    totalprice = totalprice + (2 * (t - 1) + 2.66);

                }

                else if (dessertcount != 0) {
                    var t = $('input[name="9"]:checked').length;

                    if (t > 1)
                    { totalprice = totalprice + (2 * (t - 1) + 1.66); }
                    else { totalprice = totalprice + 1.66; }
                }

                $('#price').html("Your Price is $" + Math.round(totalprice).toString()+"/person \"EXCLUSIVE TAX\". Please call us If you like the Package <br/> Please Note that this is not the Final Price. Actual Price could be same or may differ a bit(NOT MUCH) ")
                return (totalprice.toString());


            }

        }

    </script>

