    <script type="text/javascript">
        var f,n,m,o,name,countright=0,c=0;
  
        $(document).ready(function () {
            if (window.innerWidth > 767) {
                $('.top-bar').hover(function () { $(this).css('background-color', '#F75D59') }).mouseout(function () { $(this).css('background-color', 'white') });
            } $('p[id="cater-text"]').click(function () {
                $('#back-image').slideUp("slow");




            })
            $('#saver').hide();
            $('#saver').hide();
            if (window.innerWidth <= 767) {
                $('p[id="cater-text"]').css('height', '85px');
                $('img[id="buffet-image"]').css('height', '70px');
                $('.top-bar').css('background-color', '#9F000F');
                $('#saver').show();
                $('#menu-icon').click(function () {
                    $('#saver').css('height', '150px')
                    c++

                    if (c % 2 != 0) {
                        $('div[id="catering-container"]').animate({ 'top': 200 });
                    }
                    else {
                        $('div[id="catering-container"]').animate({ 'top': 0 });
                        $('#saver').css('height', '30px')
                    }

                })

            }
            $('img[id="item-image"]').each(function () {
                if ($(this).attr('class') != "1") {
                    $(this).hide();
                }
            })
            $('p[id="item-image"]').each(function () {
                if ($(this).attr('class') != "1") {
                    $(this).hide();
                }
            })

            $('li[id="mobile-list"]').each(function () {
                $(this).click(function () {
                    $('img[id="item-image"]').each(function () {
                        $(this).hide();
                    })
                    $('p[id="item-image"]').each(function () {
                        $(this).hide();
                    })
                    a = ($(this).attr('class'));
                    b = ($(this).attr('name'));

                    $('p[id="mob-category"]').html(b);
                    $('img[class=' + a + ']').each(function () {
                        $(this).show(0, "swing");
                    }).fadeOut(0).fadeIn(1500, "swing");
                    $('p[class= ' + a + ']').each(function () {
                        $(this).show(0, "swing");
                    }).fadeOut(0).fadeIn(1500, "swing");
                })

            })
            //this is to show the trays price
            $('img[id="item-image"]').each(function () {
                $(this).click(function () {
                    var e = ($(this).attr('name'));
                    $('p[name="' + e + '"]').each(function () {
                        $(this).slideToggle("slowly");
                    })

                })
            })

            $('div[class="custom"]').each(function () {
                $(this).click(function () {

                    var r = ($(this).attr('id'));
                    var k = $('div[class=' + r + ']').css('height');
                    $(this).css('height', k);
                    $('div[class=' + r + ']').each(function () {
                        $(this).css("visibility", "visible");

                    })

                })
            })
            //for custom package
            $('div[id="custom-appt"]').html($('div[class="Veg Pakora"]').html());

            $('a[name="custom"]').each(function () {
                $(this).click(function () {
                    o = $(this).attr('id');
                })
            })
            $('li[id="custom-list"]').each(function () {
                $(this).click(function () {
                    name = ($(this).attr('class'));
                    if (o == "upgrade") {
                        alert(o);
                        $('div[id="custom-appt"]').html($('div[class="' + name + '"]').html());
                    }
                    else if (o == "add-up") {
                        alert(o);

                        $('div[id="custom-appt"]').append($('div[class="' + name + '"]').html());
                    }


                })


            })
         
            $('a[id="curry"]').each(function () {
                $(this).click(function () {
                    n = $(this).attr('name');
                })

            })
            $('img[id="item-image1"]').hide(0);
            $('p[id="item-image1"]').hide(0);
            $('li[id="mobile-list1"]').each(function () {
                $(this).click(function () {
                    f = ($(this).attr('class'));
                    $('a[name= "' + n + '"]').hide(0);
                    $('#any').html(f);

                    //$('#any').html($('img[name="' + f + '"]').attr(name));
                    $('img[name="' + f + '"]').show();
                    $('p[class=' + f + ']').show();
                })


